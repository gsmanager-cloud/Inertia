{
    "name": "gsmanager-cloud/inertia",
    "type": "library",
    "description": "The GSManager adapter for Inertia.js.",
    "keywords": [
        "gsmanager",
        "inertia"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Tyuhtin",
            "email": "info@gsmanager.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Inertia\\": "src"
        },
        "files": [
            "./helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Inertia\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.4",
        "ext-json": "*",
        "gsmanager-cloud/framework": "v1",
        "symfony/console": "^6.2|^7.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.2",
        "roave/security-advisories": "dev-master",
        "orchestra/testbench": "^8.0|^9.2|^10.0",
        "mockery/mockery": "^1.3.3",
        "phpunit/phpunit": "^10.4|^11.5"
    },
    "suggest": {
        "ext-pcntl": "Recommended when running the Inertia SSR server via the `inertia:start-ssr` artisan command."
    },
    "extra": {
        "gsmanager": {
            "providers": [
                "Inertia\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
